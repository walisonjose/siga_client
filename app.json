{
  "expo": {
    "name": "Siga Client",
    "slug": "ubersiga",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/siga_icon.png",
    "splash": {
      "image": "./assets/apa.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "siga.cliente",
      "versionCode": 1,
      "softwareKeyboardLayoutMode": "pan",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "ACCESS_COARSE_LOCATION" 
      ]


    },
    
    "ios": {
      "supportsTablet": true
    }
  }
}
